<odoo>
    <data>

        <record model="ir.ui.view" id="form_list1">
            <field name="name">aging.report.view.list</field>
            <field name="model">stock.production.lot</field>
            <field name="priority">20</field>
            <field name="arch" type="xml">
                <tree>

                    <field name="name"/>
                    <field name="pr_sku"/>
                    <field name="pr_name"/>
                    <field name="pr_type"/>
                    <field name="p_qty"/>
                    <field name="cr_date"/>
                    <field name="vend"/>
                    <field name="ph"/>
                    <field name="email"/>
                </tree>
            </field>
        </record>


        <record model="ir.actions.act_window" id="action_stock_quantity_history1">
            <field name="name">Aging Report</field>
            <field name="res_model">stock.production.lot</field>
            <field name="view_mode">tree,form</field>
            <field name="view_ids" eval="[(5, 0, 0),
        (0, 0, {'view_mode': 'tree', 'view_id': ref('form_list1')},),
        (0, 0, {'view_mode': 'form', 'view_id': ref('stock.view_production_lot_form')})
        ]"/>

        </record>


        <menuitem id="purchase_history_report1"
                  action="action_stock_quantity_history1"
                  name="Aging Report"
                  parent="stock.menu_warehouse_report"
                  sequence="4"/>


    </data>
</odoo>